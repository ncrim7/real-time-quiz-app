{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        },
    {
            "src": "package.json",
            "use": "@vercel/static",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [ {
        "src": "/(.*)",
        "dest": "/"
    },
    {
        "src": "/(.*)",
        "dest": "build/$1"
    }]
}
